<script>
String.prototype.contains = function(it) { return this.indexOf(it) != -1; };
var ee_world_links = [];
$(function() {


$("a").attr("href", function(i, oldHref) {
  if (oldHref.contains("http://everybodyedits.com/games/")) {
  $(this).attr('panda-world_name', 'value');
  ee_world_links.push(oldHref);
  return oldHref;
  }
});
$('a[href^="http://everybodyedits.com/games/"]').addClass('myClass');

$('a.myClass').mouseenter(function() {
    $(this).css("background", "#F00").css("border-radius", "3px");
}).mouseleave(function() {
     $(this).css("background", "transparent").css("border-radius", "0px");
});



});

function updateLinks(names) {
alert(names);
var j = 0;
$("a").attr("href", function(i, oldHref) {
  if (oldHref.contains("http://everybodyedits.com/games/")) {
  $(this).attr('panda-world_name', names[j]);
  j++;
  return oldHref;
  }
});

}
</script>
    
    
